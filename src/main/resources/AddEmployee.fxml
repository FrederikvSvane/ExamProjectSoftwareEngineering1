<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddEmployeeFX">
   <children>
      <Label fx:id="addEmployeeLabel" layoutX="14.0" layoutY="14.0" />
      <TextField fx:id="employeeInitials" layoutX="14.0" layoutY="63.0" />
      <Button fx:id="addEmployeeButton" layoutX="175.0" layoutY="111.0" mnemonicParsing="false" onAction="#addEmployeeToList" text="Add" />
      <Button fx:id="cancelButton" layoutX="225.0" layoutY="111.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Button fx:id="deleteEmployeeButton" layoutX="105.0" layoutY="111.0" mnemonicParsing="false" onAction="#removeEmployee" text="Remove" />
      <Label fx:id="errorLabelEmployee" layoutX="14.0" layoutY="88.0" />
   </children>
</AnchorPane>
